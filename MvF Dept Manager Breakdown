##Compare the number of male managers to the number of female managers from different departments for each year, starting from 1990.
##Navigate to https://public.tableau.com/app/profile/deondre6115/viz/MvFDeptManagerBreakdown/Chart2 for visualization 

select
	d.dept_name,
    ee.gender,
    dm.emp_no,
    dm.from_date,
    dm.to_date,
    e.calendar_year,
	case
		when year(dm.to_date) >= e.calendar_year and e.calendar_year >= year(dm.from_date) Then 1
        else 0 
	end as active
from
	(Select 
		YEAR(hire_date) as calendar_year
	from
		t_employees
	group by calendar_year) e
	Cross join
    t_dept_manager dm 
    join t_departments d on dm.dept_no = d.dept_no
    join t_employees ee on dm.emp_no = ee.emp_no
order by dm.emp_no, calendar_year;
